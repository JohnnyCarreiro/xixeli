{
  "name": "nextjs-starter-with-typescript",
  "description": "A TypeScript starter for Next.js that includes all you need to start building projects",
  "version": "1.0.0",
  "private": true,
  "author": "Johnny Carreiro <johnny@johnnycarreiro.com> (johnnycarreiro.com)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "template"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^0.21.1",
    "chrome-aws-lambda": "^10.1.0",
    "faunadb": "^4.4.0",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "puppeteer-core": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "sass": "^1.38.2",
    "slugify": "^1.6.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/axios": "^0.14.0",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^16.7.9",
    "@types/puppeteer-core": "^5.4.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  }
}
